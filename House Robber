public class Solution {
    public int rob(int[] nums) {
        int len,sum1,sum2,sumi=0;
        len=nums.length;
        if(len==0) return 0;
        if(len==1) return nums[0];
        if(len==2) return Math.max(nums[0],nums[1]);
        sum1=nums[0];sum2=Math.max(nums[1],nums[0]);
        for(int i=2; i<len; i++){
            sumi=Math.max((sum1+nums[i]),sum2);
            sum1=sum2;
            sum2=sumi;
        }
        return sumi;
    }
}
